    CommandText = 
      'select'#13#10' I.NUMSELO,'#13#10' O.NUMORDEM,'#13#10' O.CODPESSOA,'#13#10' O.DATAORDEM,'#13 +
      #10' P.NOME,'#13#10' I.SERIE,'#13#10' I.CAPACIDADE,'#13#10' I.DATACARREGAMENTO,'#13#10' (se' +
      'lect DESCMARCA from TMARCA M where I.CODMARCA = M.CODMARCA) as D' +
      'ESCMARCA,'#13#10' substring(CAST(I.DATACARGA AS char(10)) FROM 7 FOR 4' +
      ') AS VENCTOTESTE'#13#10' from TORDEM O, TIORDEM I, TPESSOA P'#13#10' where O' +
