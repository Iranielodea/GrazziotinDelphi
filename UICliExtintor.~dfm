  object Qselos: TSQLQuery
    SQLConnection = DmBanco.BancoDados
    Params = <
      item
        DataType = ftInteger
        Name = 'CODPESSOA'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'select'
      '   ITE.CODPESSOA,'
      '   ITE.SERIE'
      'from TCLIEXT ITE'
      '   where ITE.CODPESSOA = :CODPESSOA')
    Left = 328
    Top = 72
    object QselosCODPESSOA: TFloatField
      FieldName = 'CODPESSOA'
    end
    object QselosSERIE: TStringField
      FieldName = 'SERIE'
      Size = 10
    end
  end
